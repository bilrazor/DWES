<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyOtherCatalog" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Colores personalizados -->
        <item name="colorPrimary">@color/my_primary_color</item>
        <item name="colorPrimaryVariant">@color/my_primary_dark_color</item>
        <item name="colorOnPrimary">@color/my_on_primary_color</item>
        <item name="android:textColor">@color/my_text_color</item>
        <item name="android:windowBackground">@color/back_ground_color</item>
        <!-- Estilo de botones personalizado -->
        <item name="materialButtonStyle">@style/MyMaterialButtonStyle</item>

    </style>

    <style name="MyMaterialButtonStyle" parent="Widget.Material3.Button">
        <item name="android:background">@drawable/my_button_background</item>
        <item name="android:textColor">@color/my_button_text_color</item>
    </style>
    <style name="MyMaterialTitleStyle" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textColor">@color/my_title_text_color</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="MyMaterialPageStyle" parent="Theme.AppCompat.Light">
        <item name="android:background">@color/my_page_color</item>

    </style>

    <style name="Theme.MyOtherCatalog" parent="Base.Theme.MyOtherCatalog" />
</resources>

